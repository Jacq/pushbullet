{
    "name": "laravel-notification-channels/pushbullet",
    "description": "Laravel 5.3+ Notifications Pushbullet Driver",
    "homepage": "https://github.com/laravel-notification-channels/pushbullet",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.us",
            "homepage": "https://alex.plekhanov.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pushbullet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pushbullet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
